{
  "name": "fine-me-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:simulator": "sh ./scripts/ios_simulator.sh",
    "ios:stg": "ENVFILE=.env.staging react-native run-ios --scheme=FineMe-stg",
    "ios:prd": "ENVFILE=.env react-native run-ios --scheme=FineMe",
    "android:stg": "ENVFILE=.env.staging react-native run-android --variant stagingDebug --appIdSuffix \"dev\"",
    "android:prd": "ENVFILE=.env react-native run-android --variant productionDebug --appIdSuffix \"prd\"",
    "ios-code-push:stg": "plutil -replace CodePushDeploymentKey -string MEJ4m9k1D5vDJsOnfoz4bMT8TmXwE2CuiUBJK ios/FineMe/Info.plist",
    "ios-code-push:prd": "plutil -replace CodePushDeploymentKey -string WPXphZTFsCnS2kLpUQmRoBD58zSiK1OzWleTF ios/FineMe/Info.plist",
    "build:android:stg": "cd android && ENVFILE=.env.staging ./gradlew app:assembleStagingRelease",
    "build:android:prd": "cd android && ENVFILE=.env ./gradlew app:assembleProductionRelease",
    "build:android:bundle": "cd android && ENVFILE=.env ./gradlew bundleRelease",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "postinstall": "patch-package",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-firebase/app": "^18.0.0",
    "@react-native-firebase/app-distribution": "^18.0.0",
    "axios": "^1.4.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "patch-package": "^7.0.0",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-code-push": "^8.0.2",
    "react-native-config": "^1.5.1",
    "react-query": "^3.39.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.3",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
